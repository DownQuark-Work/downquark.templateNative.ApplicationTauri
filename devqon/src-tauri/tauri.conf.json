{
  "app": {
    "security": {
      "csp": null
    },
    "windows": [
      {
        "decorations": true,
        "fullscreen": false,
        "height": 600,
        "resizable": true,
        "title": "DevQon",
        "visible": false,
        "width": 800
      },
      {
        "alwaysOnTop": true,
        "decorations": false,
        "height": 400,
        "label": "splashscreen",
        "url": "splash.html",
        "width": 600
      }
    ],
    "withGlobalTauri": false
  },
  "build": {
    "beforeBuildCommand": "yarn build",
    "beforeDevCommand": "yarn dev",
    "devUrl": "http://localhost:1234",
    "frontendDist": "../dist"
  },
  "bundle": {
    "active": true,
    "icon": [
      "icons/_downquark/32x32.png",
      "icons/_downquark/128x128.png",
      "icons/_downquark/128x128@2x.png",
      "icons/_downquark/icon.icns",
      "icons/_downquark/icon.ico"
    ],
    "targets": "all"
  },
  "identifier": "work.downquark.devqon",
  "plugins": {
    "shell": {
      "open": true
    }
  },
  "productName": "devqon",
  "version": "0.0.1"
}