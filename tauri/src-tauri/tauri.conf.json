{
  "build": {
    "beforeBuildCommand": "yarn build",
    "beforeDevCommand": "yarn dev",
    "devPath": "http://localhost:1234",
    "distDir": "../src-ui",
    "withGlobalTauri": false
  },
  "package": {
    "productName": "devqon",
    "version": "0.0.3-pre-release"
  },
  "tauri": {
    "allowlist": {
      "all": false
    },
    "bundle": {
      "active": true,
      "category": "DeveloperTool",
      "copyright": "downquark 2024",
      "deb": {
        "depends": []
      },
      "externalBin": [],
      "icon": [
        "icons/_downquark/32x32.png",
        "icons/_downquark/128x128.png",
        "icons/_downquark/128x128@2x.png",
        "icons/_downquark/icon.icns",
        "icons/_downquark/icon.ico"
      ],
      "identifier": "com.downquark.developmentqonsole",
      "longDescription": "",
      "macOS": {
        "entitlements": null,
        "exceptionDomain": "",
        "frameworks": [],
        "providerShortName": null,
        "signingIdentity": null
      },
      "resources": [],
      "shortDescription": "",
      "targets": "all",
      "windows": {
        "certificateThumbprint": null,
        "digestAlgorithm": "sha256",
        "timestampUrl": ""
      }
    },
    "security": {
      "csp": null
    },
    "systemTray": {
      "iconPath": "icons/_downquark/icon.png",
      "iconAsTemplate": true
    },
    "updater": {
      "active": false
    },
    "windows": [
      {
        "alwaysOnTop": true,
        "fullscreen": false,
        "height": 600,
        "resizable": true,
        "title": "DevQon",
        "width": 800,
        "visible": false
      },
      {
        "alwaysOnTop": true,
        "width": 600,
        "height": 400,
        "decorations": false,
        "url": "splash.html",
        "label": "splashscreen"
      }
    ]
  }
}
